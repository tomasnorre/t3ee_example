{
  "name": "tomasnorre/t3ee_example",
  "type": "typo3-cms-extension",
  "description": "Demo Extension for the T3EE",
  "license": ["GPL-2.0+"],
  "require": {
    "typo3/cms-core": "~6.2.14|>=7.4.0,<8.0"
  },
  "require-dev": {
    "phpunit/phpunit": "~4.7.0"
  },
  "authors": [
    {
      "name": "Tomas Norre Mikkelsen",
      "email": "tomas.mikkelsen@aoe.com",
      "role": "Developer",
      "homepage": "http://blog.random-ip.com"
    }
  ],
  "autoload": {
    "psr-4": {
      "TomasNorre\\T3ee\\": "Classes",
      "TYPO3\\CMS\\Core\\Tests\\": ".Build/vendor/typo3/cms/typo3/sysext/core/Tests/"
    }
  },
  "config": {
    "vendor-dir": ".Build/vendor",
    "bin-dir": ".Build/bin"
  },
  "scripts": {
    "post-autoload-dump": [
      "mkdir -p .Build/Web/typo3conf/ext/",
      "[ -L .Build/Web/typo3conf/ext/ext_scaffold ] || ln -snvf ../../../../. .Build/Web/typo3conf/ext/ext_scaffold"
    ]
  },
  "extra": {
    "branch-alias": {
      "dev-master": "1.0.x-dev"
    },
    "typo3/cms": {
      "cms-package-dir": "{$vendor-dir}/typo3/cms",
      "web-dir": ".Build/Web"
    }
  }
}